{
  "problemMatcher": [
    {
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+): (E\\d+): (.+) \\(([^\\)]+)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 5
        }
      ],
      "owner": "pylint-error"
    }
  ]
}
